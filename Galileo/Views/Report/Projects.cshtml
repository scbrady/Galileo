@model Galileo.ViewModels.CourseProjectsAndUsers

@{
    ViewBag.Title = "Project Page";
}

@if (Model.projects.Any())
{
   <h3>Projects</h3>
   @Html.Partial("~/Views/Shared/_Modules.cshtml", Model.projects, new ViewDataDictionary { { "Link", "Project?projectId=" } })
}

@if (Model.teams.Any())
{
   <h3>Teams</h3>
   @Html.Partial("~/Views/Shared/_Modules.cshtml", Model.teams, new ViewDataDictionary { { "Link", "Team?teamId=" } })
}

@if (Model.users.Any())
{
   <h3>Users</h3>
   @Html.Partial("~/Views/Shared/_Modules.cshtml", Model.users, new ViewDataDictionary { { "Link", "Individual?userId=" } })
}